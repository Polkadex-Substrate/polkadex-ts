{
  "name": "@polkadex/utils",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "GPL-3.0-only",
  "version": "0.0.0",
  "files": [
    "dist/**"
  ],
  "scripts": {
